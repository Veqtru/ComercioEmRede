<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_inicio">
    <fragment
        android:id="@+id/nav_inicio"
        android:name="com.example.comercioemrede.fragment.Inicio"
        android:label="@string/fragment_inicio"
        tools:layout="@layout/fragment_inicio">
        <action
            android:id="@+id/action_inicio_to_inicio_second"
            app:destination="@id/nav_inicio_second" />
    </fragment>

    <fragment
        android:id="@+id/nav_inicio_second"
        android:name="com.example.comercioemrede.fragment.InicioSecond"
        android:label="@string/inicio_second"
        tools:layout="@layout/fragment_inicio_second">
        <action
            android:id="@+id/action_inicio_second_to_inicio"
            app:destination="@id/nav_inicio" />
        <argument
            android:name="myArg"
            app:argType="string" />

    </fragment>
    <fragment
        android:id="@+id/nav_buscar"
        android:name="com.example.comercioemrede.fragment.Buscar"
        android:label="@string/fragment_buscar"
        tools:layout="@layout/fragment_buscar"/>
    <fragment
        android:id="@+id/nav_perfil"
        android:name="com.example.comercioemrede.fragment.Perfil"
        android:label="@string/fragment_perfil"
        tools:layout="@layout/fragment_perfil" />
    <fragment
        android:id="@+id/nav_historico"
        android:name="com.example.comercioemrede.fragment.Historico"
        android:label="@string/fragment_historico"
        tools:layout="@layout/fragment_historico" />
    <fragment
        android:id="@+id/nav_favoritos"
        android:name="com.example.comercioemrede.fragment.Favoritos"
        android:label="@string/fragment_favoritos"
        tools:layout="@layout/fragment_favoritos" />
</navigation>